package com.java.Practise;

public class NOD {
int n = 4536;
int i=1;
int count =0;
public void count() {
	
	for( i=1; n>10;i++)
	{
		n= n/10;
		count ++;

	}
	
  System.out.println("Total digits are "+count+1);
}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
NOD nod = new NOD();
nod.count();
	}

}
