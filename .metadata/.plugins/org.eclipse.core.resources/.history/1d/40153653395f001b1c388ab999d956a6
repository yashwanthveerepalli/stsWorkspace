package com.java.Practise;

public class NOD {
int n = 4536;
int i=0;
public void count() {
	for( i=0; n>10;i++)
	{
		n=n/10;
		
	}
  System.out.println("Total digits are "+i+1);
}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
NOD nod = new NOD();
nod.count();
	}

}
